    "description": "Alert driver for the Omnipay payment processing library",
    "keywords": [
        "alert",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/digitickets/omnipay-alert",
    "license": "MIT",
    "authors": [
        {
            "name": "Denise Ignatova",
            "email": "dignat@yahoo.com"
        }
    ],
    "autoload": {
